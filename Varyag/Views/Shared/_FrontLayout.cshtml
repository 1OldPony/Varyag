            <div style="display:flex;align-items:center;justify-content:flex-start">
                <div class="shortTopMenu">
                    <div class="navigationButton" onclick="navigationButton()"><img src="~/images/icons/бургер-бел-спис2.png" /></div>
                    <div style="display:flex;align-items:center;font-size:2.5rem;">
                        ВАРЯГ
                    </div>
                </div>
                <div id="secondVarMenu" class="fullTopMenu">
                    <a class="topMenuIcon" asp-controller="about" asp-action="index">
                        <img src="~/images/лого-кораблик-с-текстом-белый-жирный.png" />
                    </a>
                    <a href="../../../katalog" style="margin-right:15px;color:white">КАТАЛОГ</a>
                    <a id="aboutMenu" href="../../../o-nas" style="margin-right:15px;color:white">О НАС</a>
                </div>
            </div>
            <div style="display:flex;align-items:center;">
                <div style="display:flex">
                    <a style="cursor:pointer;margin-right:5px" href="https://vk.com/shipyard_varyag" target="_blank"><img src="~/images/icons/vkont.png" alt="vk" class="contactsSocial" /></a>
                    <a style="cursor:pointer;margin-right:5px" href="https://www.facebook.com/VaryagShipyard" target="_blank"><img src="~/images/icons/facebook.png" alt="facebook" class="contactsSocial" /></a>
                    <a style="cursor:pointer" href="https://www.instagram.com/varyagshipyard/?hl=ru" target="_blank"><img src="~/images/icons/insta.png" alt="facebook" class="contactsSocial" /></a>
                </div>
                <div><img src="~/images/icons/contacts2.png" class="contactsSocial" id="contacts" /></div>
                <div><img src="~/images/icons/contacts2.png" class="contactsSocial" id="contacts" /></div>
                <div><img src="~/images/icons/contacts.png" class="contactsSocial" style="display:none" /></div>
                <a id="contactsWords" style="margin-left:15px;color:white;width:auto;" href="https://varyag-onego.ru#ourContacts">КОНТАКТЫ</a>
            </div>
        </div>
    </div>
    @*<partial name="_CookieConsentPartial" />*@

    <div style="margin-bottom:50px"></div>

    @RenderBody()
