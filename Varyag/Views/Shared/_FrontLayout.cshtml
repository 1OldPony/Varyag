                    <a asp-action="SailboatsStudy" asp-controller="Catalog"><div class=topNavCategory>Учебные</div></a>
                    <a asp-action="SailboatsHistorical" asp-controller="Catalog"><div class=topNavCategory>Исторические</div></a>
