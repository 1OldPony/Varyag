@model IEnumerable<Varyag.Models.Project>
@{
    Layout = "~/Views/Shared/_FrontLayout.cshtml";
}
<div class="MyContainer">
    <input type="text" id="boatsTypeSelector" value="@ViewBag.boatsType" hidden />
    <div class="path">
        <a href="../../О-Нас">Главная</a> > <a href="../../Каталог">Каталог</a> > <a>Лодки</a>
    </div>
    <div class="largeTopFoto">
        <div class="textOverCollage">
            <h1 id="textOverCollage" class="plitkaCategoryHeader">Катера</h1>
        </div>
    </div>
    <div class="smallTopFoto">
        <h2 class="smallTopFotoText">Катера</h2>
    </div>
    <div class="myBorder"></div>
    <div class="contentBlock">
        <div class="leftMenu">
            <nav>
                <ul class="leftNavMenu">
                    <li>
                        <div class="leftNavLine"><div class="navLineHeader">Лодки</div><div id="lodkiOpen" class="leftNavOpen">+</div><div id="lodkiClose" class="leftNavClose">-</div></div>
                        <ul id="lodki-Sub" class="leftSubNavOpen">
                            <li class="leftSubCategory">
                                Народные лодки
                            </li>
                            <li class="leftSubCategory">
                                Прогулочные парусные лодки
                            </li>
                            <li class="leftSubCategory">
                                Прогулочные гребные лодки
                            </li>
                        </ul>
                    </li>
                    <li>
                        <div id="shlupkiNav" class="leftNavLine"><div class="navLineHeader">Шлюпки</div><div id="shlupkiOpen" class="leftNavOpen">+</div><div id="shlupkiClose" class="leftNavClose">-</div></div>
                        <ul id="shlupki-Sub" class="leftSubNavOpen">
                            <li class="leftSubCategory">
                                Шлюпки ЯЛ2, ЯЛ4, ЯЛ6
                            </li>
                            <li class="leftSubCategory">
                                Ботики
                            </li>
                            <li class="leftSubCategory">
                                Гребные катера и вельботы
                            </li>
                            <li class="leftSubCategory">
                                Учебные пособия
                            </li>
                        </ul>
                    </li>
                    <li>
                        <div id="kateraNav" class="leftNavLine"><div class="navLineHeader" style="color:red">Катера</div><div id="kateraOpen" class="leftNavOpenCurrent">+</div><div id="kateraClose" class="leftNavCloseCurrent">-</div></div>
                        <ul id="katera-Sub" class="leftSubNavOpenCurrent">
                            <li class="leftSubCategory">
                                Рабочие/Рыболовные катера
                            </li>
                            <li class="leftSubCategory">
                                Пассажирские катера
                            </li>
                            <li class="leftSubCategory">
                                Каютные катера
                            </li>
                            <li class="leftSubCategory">
                                Мотосейлеры
                            </li>
                        </ul>
                    </li>
                    <li>
                        <div id="ladiyNav" class="leftNavLine" style="margin-top:92px"><div class="navLineHeader">Ладьи</div><div id="ladiyOpen" class="leftNavOpen">+</div><div id="ladiyClose" class="leftNavClose">-</div></div>
                        <ul id="ladiy-Sub" class="leftSubNavOpen">
                            <li class="leftSubCategory">
                                Парусно-моторные ладьи
                            </li>
                            <li class="leftSubCategory">
                                Парусно-гребные ладьи
                            </li>
                        </ul>
                    </li>
                    <li>
                        <div id="sailboatsNav" class="leftNavLine"><div class="navLineHeader">Парусники</div><div id="sailboatsOpen" class="leftNavOpen">+</div><div id="sailboatsClose" class="leftNavClose">-</div></div>
                        <ul id="sailboats-Sub" class="leftSubNavOpen">
                            <li class="leftSubCategory">
                                Исторические парусники
                            </li>
                            <li class="leftSubCategory">
                                Учебные парусники
                            </li>
                            <li class="leftSubCategory">
                                Швертботы
                            </li>
                            <li class="leftSubCategory">
                                Парусные яхты
                            </li>
                        </ul>
                    </li>
                    <li>
                        <div id="modelsNav" class="leftNavLine"><div class="navLineHeader">Разное</div><div id="modelsOpen" class="leftNavOpen">+</div><div id="modelsClose" class="leftNavClose">-</div></div>
                        <ul id="models-Sub" class="leftSubNavOpen">
                            <li class="leftSubCategory">
                                Тренажеры
                            </li>
                            <li class="leftSubCategory">
                                Флагштоки
                            </li>
                            <li class="leftSubCategory">
                                Клумбы
                            </li>
                            <li class="leftSubCategory">
                                Кровати
                            </li>
                        </ul>
                    </li>
                </ul>
            </nav>
            <div class="ourContacts">
                <div>НАШИ КОНТАКТЫ</div>
                <div class="contactsLeft">
                    <div class="contactsUnit"><img src="~/images/icons/phone.png" class="contactsIcon" alt="cont" />&nbsp;+7 (814) 273-35-80</div>
                    <a style="cursor:pointer" href="http://www.varyag-onego.ru" target="_blank" class="contactsUnit"><img src="~/images/icons/global_114550x96.png" class="contactsIcon" />&nbsp;www.varyag-onego.ru</a>
                    <div class="contactsUnit"><img src="~/images/icons/email.png" alt="mail" class="contactsIcon" />&nbsp;varyag@onego.ru</div>
                </div>
            </div>
        </div>
        <div class="projectsHalf">
            <div class="topCatalogLine">
                <div class="categorysLineTtem">Все катера</div><div style="display:flex"><img src="~/images/icons/list3.png" style="margin-right:5px" /><img src="~/images/icons/plitca1-2.png" style="margin-right:5px" /></div>
            </div>
            @await Component.InvokeAsync("ProjectsCatalog", new { category = "boat" })
        </div>
    </div>
    <input type="text" id="currentPage" value="katera" hidden/>
</div>
